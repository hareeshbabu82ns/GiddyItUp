  "bugs": {
    "url": "https://github.com/LNoeltner1/GiddyItUp/issues"
  },
  "homepage": "https://github.com/LNoeltner1/GiddyItUp#readme",
  "dependencies": {
    "apollo-server": "^3.6.4",
    "apollo-server-core": "^3.6.4",
    "apollo-server-express": "^3.6.4",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "graphql": "^16.3.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.6",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
}
